{
  "name": "gloater-client",
  "homepage": "https://github.com/nathanjsharpe/gloater",
  "version": "0.1.0",
  "devDependencies": {
    "autoprefixer": "6.5.1",
    "babel-core": "6.17.0",
    "babel-eslint": "7.0.0",
    "babel-jest": "16.0.0",
    "babel-loader": "6.2.5",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-preset-react-app": "^1.0.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "cross-spawn": "4.0.2",
    "css-loader": "0.25.0",
    "detect-port": "1.0.1",
    "dotenv": "2.0.0",
    "enzyme": "^2.6.0",
    "eslint": "3.8.1",
    "eslint-config-react-app": "^0.3.0",
    "eslint-loader": "1.6.0",
    "eslint-plugin-flowtype": "2.21.0",
    "eslint-plugin-import": "2.0.1",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.4.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fetch-mock": "^5.5.0",
    "file-loader": "0.9.0",
    "filesize": "3.3.0",
    "find-cache-dir": "0.1.1",
    "fs-extra": "0.30.0",
    "gzip-size": "3.0.0",
    "html-webpack-plugin": "2.24.0",
    "http-proxy-middleware": "0.17.2",
    "ignore-styles": "^5.0.1",
    "json-loader": "0.5.4",
    "mocha": "^3.2.0",
    "nightwatch": "^0.9.9",
    "object-assign": "4.1.0",
    "path-exists": "2.1.0",
    "postcss-loader": "1.0.0",
    "promise": "7.1.1",
    "react-addons-test-utils": "^15.4.1",
    "react-dev-utils": "^0.3.0",
    "recursive-readdir": "2.1.0",
    "rimraf": "2.5.4",
    "selenium-standalone": "^5.9.0",
    "sinon": "^1.17.6",
    "strip-ansi": "3.0.1",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.16.2",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "1.0.0"
  },
  "dependencies": {
    "blueimp-md5": "^2.6.0",
    "lodash.get": "^4.4.2",
    "lodash.trim": "^4.5.1",
    "lodash.uniq": "^4.5.0",
    "material-ui": "^0.16.4",
    "moment": "^2.17.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "4.0.0-alpha.6",
    "react-tap-event-plugin": "^2.0.1",
    "recompose": "^0.21.0",
    "redux": "^3.6.0",
    "redux-form": "^6.3.2",
    "redux-logger": "^2.7.4",
    "redux-thunk": "^2.1.0"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "NODE_ENV=test GLOATER_API_URL=http://localhost:3000 node_modules/mocha/bin/mocha --compilers js:babel-core/register --require ignore-styles --recursive --reporter nyan 'src/**/*.spec.js'",
    "test:watch": "NODE_ENV=test GLOATER_API_URL=http://localhost:3000 node_modules/mocha/bin/mocha --compilers js:babel-core/register --compilers jsx:babel-core/register --require ignore-styles --watch --recursive --reporter nyan 'src/**/*.spec.js*'",
    "test:e2e": "NODE_ENV=test node_modules/nightwatch/bin/nightwatch",
    "test:e2e:setup": "node_modules/selenium-standalone/bin/selenium-standalone install"
  },
  "jest": {
    "moduleFileExtensions": [
      "jsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/FileStub.js",
      "^.+\\.css$": "<rootDir>/config/jest/CSSStub.js"
    },
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/(build|docs|node_modules)/"
    ],
    "testEnvironment": "node"
  },
  "babel": {
    "presets": [
      "react-app"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "babel-plugin-webpack-alias",
            {
              "config": "./config/webpack.config.dev.js"
            }
          ]
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  }
}
